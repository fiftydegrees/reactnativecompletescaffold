{
  "name": "rnscaffold",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "ios-dev": "ENVFILE=.env react-native run-ios --scheme \"rnscaffolddev\"",
    "android-dev": "ENVFILE=.env react-native run-android --variant devDebug",
    "ios-staging": "ENVFILE=.env.staging react-native run-ios --scheme \"rnscaffoldstaging\"",
    "android-staging": "ENVFILE=.env.staging react-native run-android --variant stagingDebug",
    "ios-production": "ENVFILE=.env.production react-native run-ios --scheme \"rnscaffoldproduction\"",
    "android-production": "ENVFILE=.env.production react-native run-android --variant productionDebug",
    "assemble-android-dev": "(cd android && SET ENVFILE=.env && ./gradlew assembledevDebug",
    "assemble-android-staging": "(cd android && SET ENVFILE=.env.staging && ./gradlew assemblestagingDebug",
    "assemble-android-production": "(cd android && SET ENVFILE=.env.production && ./gradlew assembleproductionRelease",
    "start": "react-native start",
    "lint": "eslint ."
  },
  "dependencies": {
    "@react-native-firebase/analytics": "^6.2.0",
    "@react-native-firebase/app": "^6.2.0",
    "@react-native-firebase/messaging": "^6.2.0",
    "react": "16.9.0",
    "react-native": "0.61.5",
    "react-native-config": "^0.12.0",
    "react-native-gesture-handler": "^1.5.3",
    "react-native-reanimated": "^2.10.0",
    "react-native-safe-area-context": "^0.6.2",
    "react-navigation": "^4.0.10"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/runtime": "^7.7.7",
    "@react-native-community/eslint-config": "^0.0.6",
    "babel-jest": "^24.9.0",
    "eslint": "^6.8.0",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.57.0",
    "react-test-renderer": "16.9.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
